a,b:inteiro
c:logico

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
c<-verdadeiro
leia(a,b)
         se(a>b) e (b>0) ou (c<>verdadeiro) entao
            escreva("Boa tarde!")
         fimse
